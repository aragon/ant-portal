  "scripts": {
    "start": "dotenv yarn sync-assets && cross-env TSC_COMPILE_ON_ERROR=true react-app-rewired start",
    "start:ethereum": "cross-env NETWORK_ENVIRONMENT=ethereum && yarn start",
    "start:rinkeby": "cross-env NETWORK_ENVIRONMENT=rinkeby && yarn start",
    "build": "dotenv yarn sync-assets && react-app-rewired build",
    "build:ethereum": "cross-env NETWORK_ENVIRONMENT=ethereum yarn build",
    "build:rinkeby": "cross-env NETWORK_ENVIRONMENT=rinkeby yarn build",
    "test": "react-app-rewired test --passWithNoTests --watchAll=false",
    "testWatch": "yarn test --watchAll=true",
    "eject": "react-scripts eject",
    "lint": "eslint --ext ts,tsx src --max-warnings 0 && yarn typecheck",
    "typecheck": "tsc",
    "sync-assets": "copy-aragon-ui-assets ./public"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test",
      "pre-commit": "yarn lint"
    }
  },
  "browserslist": [
    ">2%",
    "last 1 edge versions",
    "not ie > 0",
    "not op_mini all"
  ]
